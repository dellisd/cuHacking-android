# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'cuHacking' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for cuHacking
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Messaging'
  pod 'Firebase/Analytics'
  pod 'Fabric', '~> 1.10.2'
  pod 'Crashlytics', '~> 3.13.2'
  pod 'Mapbox-iOS-SDK'
  pod 'Swinject'
  pod 'SwiftGen', '~> 6.0'
  pod 'SwiftyJSON', '~> 4.0'



  target 'cuHackingTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Messaging'
    pod 'Firebase/Analytics'
    pod 'Fabric', '~> 1.10.2'
    pod 'Crashlytics', '~> 3.13.2'
    pod 'Mapbox-iOS-SDK'
    pod 'Swinject'
    pod 'SwiftGen', '~> 6.0'
  end

  target 'cuHackingUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Messaging'
    pod 'Firebase/Analytics'
    pod 'Fabric', '~> 1.10.2'
    pod 'Crashlytics', '~> 3.13.2'
    pod 'Mapbox-iOS-SDK'
    pod 'Swinject'

  end

end
